Cell[BoxData[{
 RowBox[{
  RowBox[{"M", "=", 
   RowBox[{"ma", "+", "mb"}]}], ";", 
  RowBox[{"\[Mu]", "=", 
   FractionBox[
    RowBox[{"ma", " ", "mb"}], "M"]}], ";", 
  RowBox[{"u", "=", 
   FractionBox[
    RowBox[{"G", " ", "M"}], "r"]}], ";", 
  RowBox[{"l", "=", 
   FractionBox["p\[Theta]", 
    RowBox[{"G", " ", "M", " ", "\[Mu]"}]]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"HPM1", "[", 
     RowBox[{"p\[Theta]_", ",", "r_"}], "]"}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{"2", "u"}]}], ")"}], 
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{
        SuperscriptBox["l", "2"], 
        SuperscriptBox["u", "2"]}]}], ")"}]}]}], ";"}], 
  RowBox[{"(*", 
